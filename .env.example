# Google Generative AI (Gemini) API Key
# Get this from: https://ai.google.dev/
GOOGLE_API_KEY=your_google_api_key_here

# BigQuery Service Account JSON path
# Download from Google Cloud Console: https://console.cloud.google.com/
BIGQUERY_SERVICE_ACCOUNT_JSON_PATH=./path/to/bigquery-service-account.json

# SAP HANA Database Connection (optional if using SAP HANA)
SAP_HANA_HOST=your_hana_host.example.com
SAP_HANA_PORT=30015
SAP_HANA_USER=your_hana_user
SAP_HANA_PASSWORD=your_hana_password
